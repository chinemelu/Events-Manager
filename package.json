{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "An application that helps admin to manage its events centres, and users to manage its events",
  "main": "app.js",
  "scripts": {
    "test": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate && cross-env NODE_ENV=test  nyc mocha --require babel-register server/test/test.js --timeout 10000 --exit",
    "migrate:dev": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "heroku-postbuild": "set NODE_ENV=production&& npm run migrate:dev && npm run build-server && npm run seed",
    "unmigrate:dev": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "migrate:test": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env test",
    "unmigrate:test": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env test",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "unseed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all",
    "server": "nodemon --watch server --exec babel-node server/http/server",
    "build-server": " node ./node_modules/babel-cli/bin/babel.js -d ./build ./server",
    "tests": "SET NODE_ENV=test && mocha --compilers js:babel-core/register server/test/**.test/*.test.js",
    "babel-node": "babel-node --presets env",
    "start": "node ./build/http/server",
    "build": "babel server -d dist",
    "startDev": "node server/dist/index.js",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chinemelu/Events-Manager.git"
  },
  "author": "Chinemelu Nwosu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chinemelu/Events-Manager/issues"
  },
  "homepage": "https://github.com/chinemelu/Events-Manager#readme",
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-loader": "^2.2.0",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.8.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.14.3",
