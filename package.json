{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "An application that helps admin to manage its events centres, and users to manage its events",
  "main": "app.js",
  "scripts": {
    "test": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate && cross-env NODE_ENV=test  nyc mocha --require babel-register server/test/test.js --timeout 10000 --exit",
    "babel-node": "babel-node --presets env",
    "start": "nodemon --exec npm run babel-node server/http/server",
    "build": "babel server -d dist",
    "heroku-postbuild": "npm run build",
    "startDev": "node server/dist/index.js",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chinemelu/Events-Manager.git"
  },
  "author": "Chinemelu Nwosu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chinemelu/Events-Manager/issues"
  },
  "homepage": "https://github.com/chinemelu/Events-Manager#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.22.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "less-loader": "^4.0.5",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "sequelize-cli": "^3.1.0",
